{
  "name": "Electric Night",
  "type": "dark",
  "colors": {
    "editor.background": "#121212",
    "editor.foreground": "#f0f0f0",
    "activityBarBadge.background": "#00ffff",
    "sideBarTitle.foreground": "#00ffff",
    "list.activeSelectionForeground": "#00ffff",
    "list.inactiveSelectionForeground": "#00ffff",
    "list.highlightForeground": "#00ffff",
    "scrollbarSlider.activeBackground": "#00ffff50",
    "editorSuggestWidget.highlightForeground": "#00ffff",
    "textLink.foreground": "#00ffff",
    "progressBar.background": "#00ffff",
    "pickerGroup.foreground": "#00ffff",
    "tab.activeBorder": "#00ffff",
    "notificationLink.foreground": "#00ffff",
    "editorWidget.resizeBorder": "#00ffff",
    "editorWidget.border": "#00ffff",
    "settings.modifiedItemIndicator": "#00ffff",
    "settings.headerForeground": "#00ffff",
    "panelTitle.activeBorder": "#00ffff",
    "breadcrumb.activeSelectionForeground": "#00ffff",
    "menu.selectionForeground": "#00ffff",
    "menubar.selectionForeground": "#00ffff",
    "editor.selectionBackground": "#00ffff40",
    "statusBarItem.remoteBackground": "#00ffff",
    "activityBar.background": "#1f1f1f",
    "titleBar.activeBackground": "#1f1f1f",
    "sideBar.background": "#1a1a1a"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4a4a4a"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF1493"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#00ffff"
        // "foreground": "#00ffff"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "name": "HTML/JSX className",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.js"
      ],
      "settings": {
        "foreground": "#feca57"
      }
    },
    {
      "name": "className values",
      "scope": [
        "meta.attribute-selector.css",
        "string.quoted.double.js",
        "string.quoted.single.js"
      ],
      "settings": {
        "foreground": "#54a0ff"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#66d9ef"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#00ffff"
      }
    }
  ]
}
